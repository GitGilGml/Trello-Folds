{
    "manifest_version": 2,
    "name": "Trello Folds",
    "version": "0.0.1",
    "icons": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "64": "icons/icon64.png",
        "128": "icons/icon128-96.png"
    },
    "permissions": [
        "storage"
    ],
    "browser_action": {
        "default_icon": "icons/icon48.png"
    },
    "description": "Trello restructured",
    "content_scripts": [{
        "css": ["styles.css"],
        "js": ["lib/jquery.js", "lib/Chart.js", "tdom.js", "trello-folds.js", "content.js"],
        "matches": ["https://trello.com/*"]
    }]
}
